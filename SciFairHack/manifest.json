{
    "name": "SciFair Hack",
    "version": "1.0.0",
    "description": "Grabs answer to questions on SciFair",
    "permissions": ["tabs"],
    "host_permissions": ["https://scifair.vladb.xyz/classes/do/assignment/*"],
    "content_scripts": [
      {
        "matches": ["https://scifair.vladb.xyz/classes/do/assignment/*"],
        "js": ["popup.js"]
      }
    ],
    "action": {
      "default_icon": {
        "16": "img/hacker.png",
        "24": "img/hacker.png",
        "32": "img/hacker.png"
      },
      "default_title": "SciFair Hack",
      "default_popup": "popup.html"
    },
    "manifest_version": 3
}
